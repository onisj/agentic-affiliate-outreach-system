{
    "environment": "development",
    "api": {
        "base_url": "http://localhost:8000",
        "debug": true,
        "timeout": 30
    },
    "database": {
        "host": "localhost",
        "port": 5432,
        "name": "affiliate_dev",
        "user": "dev_user",
        "password": "dev_password"
    },
    "monitoring": {
        "enabled": true,
        "log_level": "DEBUG",
        "metrics_interval": 60
    },
    "outreach": {
        "max_concurrent_tasks": 5,
        "retry_attempts": 3,
        "retry_delay": 60,
        "rate_limits": {
            "linkedin": 100,
            "email": 500,
            "twitter": 200
        }
    },
    "analytics": {
        "enabled": true,
        "sample_rate": 1.0,
        "storage": "local"
    },
    "security": {
        "ssl_verify": false,
        "allowed_origins": ["http://localhost:3000"],
        "jwt_secret": "dev_secret_key"
    }
} 